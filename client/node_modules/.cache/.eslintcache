ates\"\nimport FavoritesButton from \"../favoritesButton/FavoritesButton\"\nimport {DisplayBeerInfoContainer} from \"../styles/DisplayBeerInfoContainer.styled\"\nimport {ListContainer} from \"../styles/ListContainer.styled\"\nimport \"./displayBeerInfo.css\"\n\nconst DisplayBeerInfo = () => {\n  const params = useParams()\n  const {id} = params\n  const [beerData, setBeerData] = useState({})\n\n  useEffect(() => {\n    const fetchData = async () => {\n      let url = `https://api.punkapi.com/v2/beers/`\n      let res = await axios.get(`${url}${id}`)\n      setBeerData(res.data[0])\n    }\n    fetchData()\n  }, [])\n\n  return (\n    <DisplayBeerInfoContainer>\n      <h1>Beer Name</h1>\n      <p>{beerData?.name}</p>\n      <h1>Description</h1>\n      <p>{beerData?.description}</p>\n      <FavoritesButton favorite={beerData} />\n      <ListContainer>\n        <div className=\"ingredients-container\">\n          <h1>Ingredient List</h1>\n          <div className=\"ingredients-text-container\">\n            <p className=\"title\">Yeast List</p>\n            <p>{beerData?.ingredients?.yeast}</p>\n            <p className=\"title\">Malt List</p>\n            {beerData?.ingredients?.malt.map((malt) => {\n              return (\n                <div className=\"list-container\">\n                  <p key={malt?.name}>{malt?.name}</p>\n                </div>\n              )\n            })}\n            <p className=\"title\">Hops List</p>\n            <div className=\"list-container\">\n              <RemoveDuplicates beerData={beerData} />\n            </div>\n          </div>\n        </div>\n\n        <div className=\"guidelines-container\">\n          <h1>Brewing Guidline</h1>\n          <div className=\"ingredients-text-container\">\n            <p className=\"title\">Fermentation Temperature</p>\n            <p>{`${beerData?.method?.fermentation?.temp?.value} degrees ${beerData?.method?.fermentation?.temp?.unit}`}</p>\n            <p className=\"title\">Mash Temperature</p>\n            <p>{`${beerData?.method?.mash_temp[0]?.temp?.value} degrees ${beerData?.method?.mash_temp[0]?.temp?.unit}`}</p>\n            <p className=\"title\">Mash Duration</p>\n            <p>{`${beerData?.method?.mash_temp[0]?.duration} minutes`}</p>\n          </div>\n        </div>\n      </ListContainer>\n    </DisplayBeerInfoContainer>\n  )\n}\n\nexport default DisplayBeerInfo\n","/Users/trungdiec/thunder/unit1/project1/brew-master/src/components/favoritesButton/FavoritesButton.jsx",[],"/Users/trungdiec/thunder/unit1/project1/brew-master/src/components/displayHome/DisplayHome.jsx",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":15,"column":17,"nodeType":"102","endLine":15,"endColumn":44},{"ruleId":"103","severity":1,"message":"104","line":13,"column":20,"nodeType":"105","messageId":"106","endLine":13,"endColumn":22},{"ruleId":"103","severity":1,"message":"104","line":4,"column":41,"nodeType":"105","messageId":"106","endLine":4,"endColumn":43},{"ruleId":"96","replacedBy":"107"},{"ruleId":"98","replacedBy":"108"},{"ruleId":"100","severity":1,"message":"109","line":22,"column":6,"nodeType":"102","endLine":22,"endColumn":8,"suggestions":"110"},"no-native-reassign",["111"],"no-negated-in-lhs",["112"],"react-hooks/exhaustive-deps","Assignments to the 'beers' variable from inside React Hook useEffect will be lost after each render. To preserve the value over time, store it in a useRef Hook and keep the mutable value in the '.current' property. Otherwise, you can move this variable directly inside useEffect.","ArrayExpression","array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside",["111"],["112"],"React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.",["113"],"no-global-assign","no-unsafe-negation",{"desc":"114","fix":"115"},"Update the dependencies array to